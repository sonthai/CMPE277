<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="farmiotapp.cmpe277.sdthai.farmiotapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TempHumidityActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FarmManagerActivity"
            android:label="@string/title_activity_farm_manager"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FarmMaintenanceActivity"
            android:label="@string/title_activity_farm_maintenance"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>